{{#sprites}}
.ic-{{name}} {
    display: block;
    width: {{px.width}};
    height: {{px.height}};
    background-image: url(../images/{{{escaped_image}}});
    background-position: {{px.offset_x}} {{px.offset_y}};
    flex-shrink: 0;
}
@mixin ic-{{name}}() {
    background-position: {{px.offset_x}} {{px.offset_y}} !important;
}
@mixin ic-{{name}}-mixin() {
    display: block;
    width: {{px.width}};
    height: {{px.height}};
    background-image: url(../images/{{{escaped_image}}});
    background-position: {{px.offset_x}} {{px.offset_y}};
    flex-shrink: 0;
}
{{/sprites}}